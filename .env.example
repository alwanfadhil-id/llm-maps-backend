# Google Maps API Configuration
# Get your API key from Google Cloud Console
GOOGLE_MAPS_API_KEY=your_server_api_key_here
GOOGLE_MAPS_CLIENT_KEY=your_client_api_key_here

# Server Configuration
PORT=3001
NODE_ENV=development

# Security
API_RATE_LIMIT=100
ALLOWED_ORIGINS=http://localhost:3001,http://127.0.0.1:3001

# Application Configuration
MAX_RESULTS=10
MAX_RETRIES=3
LLM_TIMEOUT=30000

# Open WebUI Configuration
OPEN_WEBUI_API_KEY=your_open_webui_api_key_here
OPEN_WEBUI_MODEL=llama3

# Additional API Key (for client validation, optional)
CLIENT_API_KEY=your_client_api_key_if_used
# Google Maps API Configuration
GOOGLE_MAPS_API_KEY=your_google_maps_server_key_here
GOOGLE_MAPS_CLIENT_KEY=your_google_maps_client_key_here

# Server Configuration
PORT=3001
NODE_ENV=development

# Security
API_RATE_LIMIT=100
ALLOWED_ORIGINS=http://localhost:3001,http://127.0.0.1:3001

# Application Configuration
MAX_RESULTS=10
MAX_RETRIES=3
LLM_TIMEOUT=30000

# Open WebUI Configuration
# Note: Set this to your actual Open WebUI API key when you set up Open WebUI
OPEN_WEBUI_API_KEY=sk-your_open_webui_api_key_here
OPEN_WEBUI_MODEL=llama2:7b

# Additional API Key (for client validation, optional)
CLIENT_API_KEY=your_client_api_key_here